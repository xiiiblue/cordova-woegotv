<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sitech.woegotvdebug" version="1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>name</name>
    <description>
        description
    </description>
    <author email="guolei@si-tech.com.cn" href="www.si-tech.com.cn">
        SI-TECH
    </author>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~5.1.1" />
    <content src="http://127.0.0.1:10001/woegotv/index-debug.html" />
    <allow-navigation href="http://*/*" />
    <preference name="Fullscreen" value="true" />
    <icon src="res/icon.png" />
</widget>
